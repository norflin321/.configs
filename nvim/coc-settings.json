{
  "list.height": 30,
  "list.maxPreviewHeight": 25,
  "suggest.noselect": false,
  "suggest.defaultSortMethod": "alphabetical",
  "suggest.autoTrigger": "none",
  "suggest.snippetsSupport": false,
  "signature.preferShownAbove": true,
  "diagnostic.checkCurrentLine": true,
	"diagnostic.errorSign": "‚ùå",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.virtualText": false,

  "javascript.suggestionActions.enabled": false,
  "tsserver.trace.server": "off",
  "typescript.suggest.completeFunctionCalls": false,
	"typescript.preferences.quoteStyle": "double",
	"typescript.format.placeOpenBraceOnNewLineForFunctions": true,
	"typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
	"typescript.format.semicolons": "insert",
	"typescript.workspaceSymbols.scope": "currentProject",
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,

  "rust-analyzer.inlayHints.enable": false,
	"rust-analyzer.inlayHints.bindingModeHints.enable": false,
	"rust-analyzer.inlayHints.chainingHints.enable": false,
	"rust-analyzer.inlayHints.closingBraceHints.enable": false,
	"rust-analyzer.inlayHints.closureReturnTypeHints.enable": false,
	"rust-analyzer.inlayHints.lifetimeElisionHints.enable": false,
	"rust-analyzer.inlayHints.typeHints.enable": false,
	"rust-analyzer.inlayHints.parameterHints.enable": false,

	"pyright.inlayHints.functionReturnTypes": false,
	"pyright.inlayHints.variableTypes": false,
	"python.analysis.typeCheckingMode": "off",

	"coc.preferences.formatOnSaveFiletypes": ["vue", "prisma", "go", "rust"],
	"languageserver.disableSnippetCompletion": true,
	"dialog.floatBorderHighlight": "CocFloatingBorder",
	"typescript.preferences.importModuleSpecifier": "non-relative"
}
